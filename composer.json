{
  "version": "0.3.2",
  "name": "whatarmy/fedex-rest",
  "description": "New FedEx Rest API wrapper",
  "keywords": [
    "shipping",
    "fedex",
    "rest",
    "api"
  ],
  "type": "library",
  "license": "MIT",
  "support": {
    "issues": "https://github.com/WhatArmy/FedexRest/issues"
  },
  "authors": [
    {
      "name": "WhatArmy",
      "email": "c2p@whatarmydev.com",
      "homepage": "https://whatarmy.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=8.0",
    "ext-json": "*",
    "guzzlehttp/guzzle": "6.5.5",
    "nesbot/carbon": "^2.43",
    "spatie/ray": "^1.14"
  },
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "FedexRest\\": "src/FedexRest/",
      "FedexRest\\Tests\\": "tests/FedexRest/Tests"
    },
    "classmap": [
      "src/"
    ]
  },
  "require-dev": {
    "phpunit/phpunit": "9.5"
  },
  "config": {
    "sort-packages": true
  }
}
